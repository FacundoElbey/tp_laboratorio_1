#include "Funciones.h"

float CalcularPrecioDebito(float precio)
{
	float resultado;
	resultado = 0;

	resultado = precio - precio * 0.10;

	return resultado;
}

int calcularPrecioCredito(float precioIngresado, float* precioFinal)
{
	int retorno;
	float bufferResultado;

	bufferResultado = 0;
	retorno = -1;

	bufferResultado = precioIngresado + precioIngresado * 0.25;
	&precioFinal = bufferResultado;

	return retorno;
}


